{
  "version": 2,
  "builds": [
    {
      "src": "public/index.html",
      "use": "@vercel/static"
    }
  ],
  "rewrites": [
    {
      "source": "/get_answer",
      "destination": "/index.js"
    },
    {
      "source": "/api/.*",
      "destination": "/api/index.js"
    },
    {
      "source": "/(.*)",
      "destination": "/index.js"
    }
  ]
}
